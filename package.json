{
  "name": "@developerjamey/langchain-discord",
  "version": "0.0.6",
  "description": "Discord chat log loader for LangChainJS",
  "main": "dist/discord_loader.js",
  "types": "dist/discord_loader.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "type": "module",
  "scripts": {
    "build": "tsup src/discord_loader.ts --dts --format esm,cjs --out-dir dist",
    "dev": "tsup src/discord_loader.ts --watch --onSuccess 'npm test'",
    "prepare": "npm run build",
    "test": "jest --passWithNoTests",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,json,md}'",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "@langchain/core": ">=0.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@langchain/core": "^0.2.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.5",
    "tsup": "^8.5.0",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.46.2"
  },
  "bugs": {
    "url": "https://github.com/catturtle123/langchain-discord-document-loader/issues"
  },
  "homepage": "https://github.com/catturtle123/langchain-discord-document-loader#readme",
  "author": "Jamey (김준환)",
  "license": "MIT",
  "keywords": [
    "discord",
    "langchain",
    "LangChainJS"
  ]
}
